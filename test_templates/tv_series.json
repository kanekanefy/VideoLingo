    "api": {
      "model": "deepseek-v3",
      "max_workers": 8
    },
    "whisper": {
      "model": "large-v3",
      "runtime": "cloud"
    },
    "tts_method": "sf_cosyvoice2",
    "burn_subtitles": true,
    "subtitle": {
      "max_length": 45,
      "target_multiplier": 1.15
    },
    "batch_processing": true,
    "character_consistency": true,
    "episode_numbering": true,
    "terminology_shared": true,
    "quality_threshold": 0.8
  }
}